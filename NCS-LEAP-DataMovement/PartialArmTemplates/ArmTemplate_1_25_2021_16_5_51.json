				"folder": {
					"name": "TASOrders"
				},
				"annotations": [],
				"type": "Binary",
				"typeProperties": {
					"location": {
						"type": "AmazonS3Location",
						"bucketName": "leapdata-tas",
						"folderPath": "TASOrdersQA02/Upload"
					}
				}
			},
			"dependsOn": []
