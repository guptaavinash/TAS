{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "NCS-LEAP-DataMovement"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/dfdatafoundationLink')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"sasUri": "https://dfdatafoundation.blob.core.windows.net/cntleap?sv=2019-10-10&st=2021-01-08T07%3A01%3A47Z&se=2021-01-09T07%3A01%3A47Z&sr=c&sp=racwl&sig=1zsFZyEMz%2FhOmSvt2Sf%2F2Q0wgaja5Lsoq7W4SgK6uRw%3D"
				}
			},
			"dependsOn": []
		}
	]
}