{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "NCS-LEAP-DataMovement"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/dfdatafoundationLink')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"sasUri": "https://dfdatafoundation.blob.core.windows.net/cntleap?sv=2019-12-12&st=2021-01-12T10%3A06%3A55Z&se=2022-01-13T10%3A06%3A00Z&sr=c&sp=racwl&sig=9xRnAqD1yeNDDGYEaWCExbezuNd9yr3PBLHeDQuRjPk%3D"
				}
			},
			"dependsOn": []
		}
	]
}